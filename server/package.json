{
  "name": "voting-system-backend",
  "version": "1.0.0",
  "description": "Student Voting System Backend API with Blockchain Integration",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:network": "set HOST=0.0.0.0&& set SERVER_HOST=0.0.0.0&& nodemon server.js",
    "start:network": "set HOST=0.0.0.0&& set SERVER_HOST=0.0.0.0&& node server.js",
    "prod": "set NODE_ENV=production&& node server.js",
    "prod:network": "set NODE_ENV=production&& set HOST=0.0.0.0&& set SERVER_HOST=0.0.0.0&& node server.js",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint .",
    "db:setup": "node scripts/setup-database.js",
    "db:reset": "node scripts/reset-database.js",
    "health": "curl -f http://localhost:5000/api/health || echo \"Server not ready\"",
    "health:network": "node scripts/network-health.js",
    "blockchain-status": "curl -f http://localhost:5000/api/blockchain-status || echo \"Blockchain not ready\"",
    "network:info": "node scripts/network-info.js",
    "network:start": "node scripts/network-start.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.11.5",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^2.1.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.16",
    "eslint": "^9.17.0",
    "nodemon": "^3.1.7"
  },
  "keywords": [
    "voting",
    "blockchain",
    "ethereum",
    "backend",
    "api",
    "student",
    "election"
  ],
  "author": "SSC Voting System",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
