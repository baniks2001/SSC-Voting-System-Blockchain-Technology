    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.452.0",
    "mammoth": "^1.11.0",
    "mysql2": "^3.11.5",
    "ngrok": "^5.0.0-beta.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "solc": "^0.8.28",
    "web3": "^4.16.0",
    "web3modal": "^1.9.12",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
