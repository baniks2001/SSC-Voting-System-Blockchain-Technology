{
  "name": "ssc-voting-blockchain",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "node1": "cd blockchain && start-node1.bat",
    "node2": "cd blockchain && start-node2.bat",
    "start-nodes": "start npm run node1 && timeout 5 && start npm run node2",
    "server": "node server.js",
    "start": "concurrently \"npm run start-nodes\" \"npm run server\"",
    "compile-contract": "node scripts/compile.js",
    "deploy-contract": "node scripts/deploy.js",
    "test-contract": "node scripts/test-contract.js",
    "setup-contract": "npm run compile-contract && npm run deploy-contract",
    

     "electron": "electron electron-main.cjs",
    "electron-dev": "electron electron-main.cjs --inspect",
    "quick-start": "quick-start.bat",
    "start-electron": "start-electron.bat",
    "start-all": "npm run electron",
    
    "dev:network": "cd server && npm run dev:network",
    "dev:local": "cd server && npm run dev"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
