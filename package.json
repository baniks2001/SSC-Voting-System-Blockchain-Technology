    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/web3": "^1.2.2",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "electron": "^22.0.0",
    "eslint": "^9.17.0",
